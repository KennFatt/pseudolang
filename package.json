{
    "name": "pseudolang",
    "displayName": "pseudolang",
    "description": "Pseudo-code highlighting for Visual Studio code.",
    "version": "0.0.2",
    "publisher": "kennfatt",
    "icon": "assets/logo.png",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pseudolang",
            "aliases": ["pseudocode", "pseudo-code", "pseudolang"],
            "extensions": [".pseudocode", ".pseudo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pseudolang",
            "scopeName": "source.pseudolang",
            "path": "./syntaxes/pseudolang.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KennFatt/pseudolang.git"
    }
}